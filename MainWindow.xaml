        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProxyPattern"
        mc:Ignorable="d"
        Title="Заказы и Статусы" Height="400" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Text="Заказы" FontSize="20"/>
        <DataGrid x:Name="dg" Grid.Row="1" FontSize="20"/>
    </Grid>
</Window>
